{
  "name": "tree-sitter-html",
  "version": "0.16.0",
  "description": "HTML grammar for tree-sitter",
  "main": "index.js",
  "keywords": [
    "parser",
    "lexer"
  ],
  "authors": [
    "Max Brunsfeld <maxbrunsfeld@gmail.com>",
    "Ashi Krishnan <queerviolet@github.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tree-sitter-cli": "^0.16.9"
  },
  "scripts": {
    "build": "tree-sitter generate",
    "test": "npm-run-all -s build test:*",
    "test:1": "tree-sitter test",
    "test:2": "tree-sitter parse examples/*.html --quiet --time"
  },
  "tree-sitter": [
    {
      "scope": "text.html.basic",
      "file-types": [
        "html"
      ],
      "injection-regex": "html"
    }
  ]
}
